<div class="item-news node-saved article">
    <div class="picture">
        <figure class="">{{ fields.field_image.content }}</figure>
        <div class="entity save saved" data-id="{{ fields.nid.content }}">
            <a href="#" class="node_saved">Guardar</a>
        </div>
    </div>
    <div class="text">
        <div class="category">{{ fields.field_categoria.content }}</div>
        <h3 class="title">{{ fields.title.content }}</h3>
        <div class="bottom">
            <div class="info">
                <span class="author">{{ fields.field_nombre.content }}</span>
                <date>{{ fields.created.content }}</date>
            </div>
            <div class="explorer">{{ fields.nothing.content }}</div>
        </div>
    </div>
</div>